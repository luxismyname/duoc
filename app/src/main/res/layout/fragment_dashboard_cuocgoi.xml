                        app:layout_constraintTop_toBottomOf="@id/tvRpTongCuocGoiTitle" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
